Sub forLoop()

   'Loop through the first 10 rows
   ' define array of number of chicken nuggets
   ' define string1 and string 2
   ' loop

   For i = 1 To 10
       Cells(i, 1).Value = "I will eat"
       Cells(i, 2).Value = i + 9
       Cells(i, 3).Value = "Chicken Nuggets"


   Next i


End Sub
